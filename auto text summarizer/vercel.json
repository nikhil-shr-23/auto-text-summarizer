{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "50mb",
          "includeFiles": ["templates/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ]
  }
