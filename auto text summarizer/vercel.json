{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "300mb",
          "includeFiles": [
            "templates/**",
            "requirements.txt"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ]
  }
